#!groovy
Map pipelineConfig = [
    role_name : params.role_name,
]

def getlist(){
    return ["one", "two"]
}

pipeline {

    agent {
        label 'Master'
    }

    parameters {
        choice(
            name: 'role',
            choices: getlist(),
            description: 'list of all the role names'
    )
    }

    stages {
        stage('set build name and description') {
            steps {
                script {
                    currentBuild.displayName = "creating ec2: ${params.my_ec2_tag}"
                }
            }
        }
    }
}